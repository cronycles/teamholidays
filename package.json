{
    "name": "holidays-calendar",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@craco/craco": "^6.3.0",
        "@testing-library/jest-dom": "^5.11.4",
        "@testing-library/react": "^11.1.0",
        "@testing-library/user-event": "^12.1.10",
        "dayjs": "^1.10.7",
        "react": "^17.0.2",
        "react-cookie": "^4.1.1",
        "react-dom": "^17.0.2",
        "react-scripts": "4.0.3"
    },
    "scripts": {
        "restore": "npm install && npm install -g serve json-server",
        "run:server": "json-server --watch src/api/db.json --port 4000",
        "run:local": "craco start",
        "build": "craco build",
        "run:local-sshlegacy": "craco --openssl-legacy-provider start",
        "build-sshlegacy": "craco --openssl-legacy-provider build",
        "run:production": "serve -s build"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@tailwindcss/forms": "^0.3.3",
        "autoprefixer": "^9.8.8",
        "postcss": "^7.0.39",
        "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17"
    }
}
